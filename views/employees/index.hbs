{{#extends "base"}}
{{#block "content"}}
<h1>Employees</h1>
<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Department ID</th>
        <th>Gender</th>

    </tr>
    {{#each employees}}
    <tr>
        <td>{{Employees.first_name}}</td>
        <td>{{Employees.last_name}}</td>
        <td>{{Employees.department_id}}</td>
        <td>{{Employees.gender}}</td> 

    </tr>
    {{/each}}
</table>
{{/block}}
{{/extends}}

